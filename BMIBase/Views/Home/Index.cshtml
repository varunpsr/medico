@model IEnumerable<BMIBase.Models.BodyComposition>

@{
	ViewBag.Title = "Index";
	Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
	@Html.ActionLink("Create New", "Create")
</p>
<table class="table">
	<tr>
		<th>
			@Html.DisplayNameFor(model => model.BMI)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.MetabolicRate)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.FatPercentage)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.MuscleMass)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.WaterPercentage)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.BoneDensity)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.FatWeight)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.MusclePercentage)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.BodyAge)
		</th>
	</tr>

	@foreach (var item in Model)
	{
		<tr>
			<td>
				@Html.DisplayFor(modelItem => item.BMI)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.MetabolicRate)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.FatPercentage)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.MuscleMass)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.WaterPercentage)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.BoneDensity)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.FatWeight)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.MusclePercentage)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.BodyAge)
			</td>
		</tr>
	}

</table>
